{
  "name": "touchless-gaze",
  "version": "1.0.0",
  "description": "Use Mediapipe facemesh model to capture direction a person's head is looking.",
  "main": "index.js",
  "dependencies": {
    "@tensorflow-models/facemesh": "0.0.4",
    "@tensorflow/tfjs-backend-wasm": "^2.1.0",
    "@tensorflow/tfjs-backend-webgl": "^2.1.0",
    "@tensorflow/tfjs-converter": "^2.1.0",
    "@tensorflow/tfjs-core": "^2.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "parcel ./src/index.html --no-hmr --target browser --out-dir ./dev",
    "build": "parcel build ./src/index.html --target browser --out-dir ../../docs/gaze --public-url ./"
  },
  "keywords": [
    "facemesh",
    "touchless"
  ],
  "browser": {
    "crypto": false
  },
  "browserslist": [
    "last 2 years"
  ],
  "author": "Jason Alderman (Valtech)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "babel-plugin-external-helpers": "^6.22.0",
    "clang-format": "~1.2.2",
    "parcel-bundler": "^1.12.4"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "require-jsdoc": 0,
      "valid-jsdoc": 0
    },
    "env": {
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  },
  "eslintIgnore": [
    "dist/"
  ]
}
